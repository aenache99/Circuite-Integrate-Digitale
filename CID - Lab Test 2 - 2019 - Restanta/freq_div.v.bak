module freq_div(input wire clk,
					input wire rst_n,
					input wire en,
					output reg out);
					
always @(negedge clk)
begin 
	if (rst_n == 1)
		out <=0;
	else 
		begin 
			case (en)
			01: out <= out + 2;
			10: out <= out + 4;
			11: out <= out + 8;
			default: out <= out;
		end
end
endmodule 